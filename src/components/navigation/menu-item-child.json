{
  "collectionName": "components_navigation_menu_item_children",
  "info": {
    "displayName": "Menu Item Child",
    "icon": "list",
    "description": "Child menu item that can link to either an external URL or an internal page (without further nesting)"
  },
  "attributes": {
    "label": {
      "type": "string",
      "required": true
    },
    "type": {
      "type": "enumeration",
      "required": true,
      "default": "internal",
      "enum": [
        "external",
        "internal"
      ]
    },
    "url": {
      "type": "string",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "type"
            },
            "external"
          ]
        }
      }
    },
    "page": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::page.page",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "type"
            },
            "internal"
          ]
        }
      }
    },
    "openInNewTab": {
      "type": "boolean",
      "default": false
    }
  }
}
